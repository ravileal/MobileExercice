            android:name="com.example.mobileexercice.ActivityPrincipal"
            android:label="@string/title_activity_inicio"
            android:theme="@style/Theme.MobileExercice.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mobileexercice.ActivityCadastro_2"
            android:label="@string/title_activity_tela_cadastro2"
            android:theme="@style/Theme.MobileExercice.NoActionBar" />
        <activity
            android:name="com.example.mobileexercice.ActivityCadastro"
            android:label="@string/app_name"
            android:theme="@style/Theme.MobileExercice.NoActionBar" />
    </application>

</manifest>